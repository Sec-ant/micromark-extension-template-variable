{
  "name": "micromark-extension-template-variable",
  "description": "Micromark extension to support template variables",
  "private": false,
  "version": "0.1.0",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/cjs/index.js",
  "module": "./dist/es/index.js",
  "exports": {
    ".": {
      "import": "./dist/es/index.js",
      "require": "./dist/cjs/index.js",
      "default": "./dist/es/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Sec-ant/micromark-extension-template.git"
  },
  "homepage": "https://github.com/Sec-ant/micromark-extension-template",
  "bugs": {
    "url": "https://github.com/Sec-ant/micromark-extension-template/issues",
    "email": "zezhengwu@proton.me"
  },
  "keywords": [
    "micromark",
    "micromark-extension",
    "template",
    "variable",
    "template-variable",
    "curly-brace",
    "markdown",
    "unified"
  ],
  "author": {
    "name": "Ze-Zheng Wu"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "update-hooks": "simple-git-hooks",
    "clean": "rimraf dist",
    "lint": "biome lint .",
    "format:prettier": "pretty-quick",
    "format:biome": "biome format . --write",
    "format": "pnpm -s format:prettier && pnpm -s format:biome",
    "type-check": "tsc --noEmit",
    "check:biome": "biome check --write .",
    "check": "pnpm -s format:prettier && pnpm -s check:biome",
    "prebuild": "pnpm -s check && pnpm -s type-check",
    "dev": "vite",
    "build:es": "vite build",
    "build:cjs": "tsx ./scripts/build-cjs.ts",
    "build": "conc \"pnpm:build:es\" \"pnpm:build:cjs\"",
    "postbuild:es": "tsc --declarationDir ./dist/es",
    "postbuild:cjs": "tsc --declarationDir ./dist/cjs",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "prepublishOnly": "pnpm -s build"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.6",
    "@changesets/cli": "^2.29.7",
    "@types/node": "^24.7.2",
    "@vitest/ui": "^3.2.4",
    "concurrently": "^9.2.1",
    "lint-staged": "^16.2.4",
    "micromark": "^4.0.2",
    "micromark-util-types": "^2.0.2",
    "prettier": "^3.6.2",
    "pretty-quick": "^4.2.2",
    "rimraf": "^6.0.1",
    "simple-git-hooks": "^2.13.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vite": "^7.1.10",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "micromark-util-symbol": "^2.0.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@10.18.3+sha512.bbd16e6d7286fd7e01f6b3c0b3c932cda2965c06a908328f74663f10a9aea51f1129eea615134bf992831b009eabe167ecb7008b597f40ff9bc75946aadfb08d"
}
